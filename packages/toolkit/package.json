{
  "name": "@yunlefun/toolkit",
  "type": "module",
  "version": "0.1.2-beta.3",
  "description": "Toolkit for YunLeFun developing.",
  "author": {
    "name": "YunLeFun",
    "email": "i@yunle.fun",
    "url": "https://www.yunle.fun"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/YunLeFun",
  "homepage": "https://github.com/YunLeFun/toolkit/blob/main/packages/toolkit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YunLeFun/toolkit.git"
  },
  "bugs": "https://github.com/YunLeFun/toolkit/issues",
  "keywords": [
    "toolkit",
    "utils",
    "vueuse"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@yunlefun/utils": "workspace:*",
    "@yunlefun/vueuse": "workspace:*"
  }
}
